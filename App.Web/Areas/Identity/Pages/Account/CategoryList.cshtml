@page
@model App.Web.Areas.Identity.Pages.Account.CategoryListModel
@{
}





<div class="container">
    <h2 class="text-center">Category List</h2>
    <table class="table table-bordered">
        <thead>
            <tr>
                <th>Category ID</th>
                <th>Category Name</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var category in Model.CategoryResult)
            {
                <tr>
                    <td>@category.Id</td>
                    <td>@category.CategoryName</td>
                </tr>
            }
        </tbody>
    </table>
</div>
